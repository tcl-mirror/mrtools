    externalop void ShippingCompany_shipmentReadyForPickup {shipmentID MRT_InstId} {
        printf("%s:\n", __func__) ;
        printf("    shipmentID = %u\n", shipmentID) ;
    
        union {
            MRT_EventParams params ;
            struct bookstore_Shipment_trackingNumberAssigned__EPARAMS tnaparams ;
        } tnaevent = {
