#include "testdomain.c"

<<test functions>>

<<suite tests>>

static CU_SuiteInfo suites[] = {
    <<test suites>>
    CU_SUITE_INFO_NULL,
} ;

int
main(
    int argc,
    char **argv)
{
    testdomain_initialize() ;

    CU_initialize_registry() ;

    CU_register_suites(suites) ;

    // CU_basic_set_mode(CU_BRM_VERBOSE) ;
    CU_basic_run_tests() ;

